{% for host in groups["overcloud"] %}
Host {{ host }}
    HostName {{ hostvars[host].ansible_ssh_host }}
    IdentityFile files/id_rsa_overcloud
    IdentitiesOnly yes
    User heat-admin
    StrictHostKeyChecking no
    ForwardAgent yes
    UserKnownHostsFile=/dev/null
    ProxyCommand ssh -o UserKnownHostsFile=/dev/null -o StrictHostKeyChecking=no -o ConnectTimeout=60 -i ~/.ssh/id_rsa -F ssh.config.ansible stack@{{ hostvars[groups['undercloud'][0]].ansible_ssh_host }}
{%endfor %}
